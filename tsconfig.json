{
  "compilerOptions": {
    "declaration": false,
    "declarationMap": false,
    "esModuleInterop": true,
    "importHelpers": true,
    "jsx": "preserve",
    "moduleResolution": "node",
    "outDir": "dist",
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": false,
    "strict": true,
    "types": ["node", "cheerio", "@docusaurus/module-type-aliases"]
  },
  "include": ["src"],
  "exclude": ["**/*.spec.ts"]
}
